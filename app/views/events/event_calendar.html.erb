<div class="container">
  <div class="row">
    <div class="col-md-10 mx-md-auto">
      <div id="calendar"></div>
    </div>
  </div>
</div>

<script>

  $(document).ready(function() {

    $('#calendar').fullCalendar({
      header: {
        left: 'prev,next today',
        center: 'title',
        right: 'month,basicWeek,basicDay'
      },
      defaultDate: '2019-02-15',
      navLinks: true,
      editable: true,
      eventLimit: true,
      events: '/events/events_for_calendar.json'
    });

  });

</script>